{
    "name": "laneflow/laneflow",
    "type": "library",
    "authors": [
        {
            "name": "qtvhao",
            "email": "qtvhao@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laneflow\\Laneflow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laneflow\\Laneflow\\Example\\": "example",
            "Laneflow\\Laneflow\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/routing": "^8.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.0",
        "illuminate/container": "^8.0.0 || ^9.0.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "description": "A swimlane (or swimlane diagram) is used in process flow diagrams, or flowcharts, that visually distinguishes job sharing and responsibilities for sub-processes of a business process. Swimlanes may be arranged either horizontally or vertically."
}
